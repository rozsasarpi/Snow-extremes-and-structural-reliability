@Article{Anderegg2010,
  Title                    = {Expert credibility in climate change},
  Author                   = {Anderegg, William R. L. and Prall, James W. and Harold, Jacob and Schneider, Stephen H.},
  Journal                  = {Proceedings of the National Academy of Sciences},
  Year                     = {2010},
  Number                   = {27},
  Pages                    = {12107-12109},
  Volume                   = {107},

  Abstract                 = {Although preliminary estimates from published literature and expert surveys suggest striking agreement among climate scientists on the tenets of anthropogenic climate change (ACC), the American public expresses substantial doubt about both the anthropogenic cause and the level of scientific agreement underpinning ACC. A broad analysis of the climate scientist community itself, the distribution of credibility of dissenting researchers relative to agreeing researchers, and the level of agreement among top climate experts has not been conducted and would inform future ACC discussions. Here, we use an extensive dataset of 1,372 climate researchers and their publication and citation data to show that (i) 97–98% of the climate researchers most actively publishing in the field surveyed here support the tenets of ACC outlined by the Intergovernmental Panel on Climate Change, and (ii) the relative climate expertise and scientific prominence of the researchers unconvinced of ACC are substantially below that of the convinced researchers.},
  Doi                      = {10.1073/pnas.1003187107},
  Owner                    = {Arpi},
  Timestamp                = {2015.10.24},
  Type                     = {Journal Article},
  Url                      = {http://www.pnas.org/content/107/27/12107.abstract}
}

@Article{Ando2007,
  Title                    = {Bayesian Predictive Information Criterion for the Evaluation of Hierarchical {B}ayesian and Empirical Bayes Models},
  Author                   = {Ando, Tomohiro},
  Journal                  = {Biometrika},
  Year                     = {2007},
  Number                   = {2},
  Pages                    = {443-458},
  Volume                   = {94},

  Abstract                 = {The problem of evaluating the goodness of the predictive distributions of hierarchical Bayesian and empirical Bayes models is investigated. A Bayesian predictive information criterion is proposed as an estimator of the posterior mean of the expected loglikelihood of the predictive distribution when the specified family of probability distributions does not contain the true distribution. The proposed criterion is developed by correcting the asymptotic bias of the posterior mean of the loglikelihood as an estimator of its expected loglikelihood. In the evaluation of hierarchical Bayesian models with random effects, regardless of our parametric focus, the proposed criterion considers the bias correction of the posterior mean of the marginal loglikelihood because it requires a consistent parameter estimator. The use of the bootstrap in model evaluation is also discussed.},
  Doi                      = {10.2307/20441383},
